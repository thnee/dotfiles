export EDITOR=vim

bindkey "^[Oc" forward-word   # control + right arrow
bindkey "^[Od" backward-word  # control + left arrow

alias h="hostname"
alias p="pwd"
alias u="whoami"
alias g="git"
alias dc="docker compose"
alias dk="docker"
alias m="micro"
alias j="just"
alias tf="terraform"
alias tg="terragrunt"

export VIRTUAL_ENV_DISABLE_PROMPT=1
